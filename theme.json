{
	"settings": {
		"layout": {
			"wideSize": "1020px"
		},
		"typography": {
			"fontSizes": [
				{
					"name": "Small",
					"size": "18px",
					"slug": "small"
				},
				{
					"name": "Normal",
					"size": "18px",
					"slug": "normal"
				},
				{
					"fluid": {
						"max": "20px",
						"min": "26px"
					},
					"name": "Medium",
					"size": "26px",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "36px",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "48px",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "74px",
						"min": "40px"
					},
					"name": "2X Large",
					"size": "74px",
					"slug": "xx-large"
				},
				{
					"name": "XS",
					"size": "16px",
					"slug": "x-small"
				}
			]
		}
	},
	"styles": {
		"color": {
			"text": "var(--wp--preset--color--theme-5)"
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--normal)",
			"fontWeight": "400"
		},
		"elements": {
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "700"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "1"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": {
						"ref": "styles.elements.h1.typography.fontWeight"
					},
					"lineHeight": "1"
				}
			}
		},
		"blocks": {
			"core/quote": {
				"border": {
					"style": "solid",
					"width": "0 0 0 2px",
					"color": "currentColor"
				},
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--20)",
					"margin": {
						"left": "0",
						"right": "0"
					},
					"padding": {
						"top": "var(--wp--preset--spacing--10)",
						"right": "var(--wp--preset--spacing--20)",
						"bottom": "var(--wp--preset--spacing--10)",
						"left": "var(--wp--preset--spacing--20)"
					}
				}
			}
		}
	},
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json"
}